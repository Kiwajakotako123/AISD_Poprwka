def selection_sort(tab):
    n = len(tab)
    
    for i in range(n):
        min = i
        for j in range(i+1, n):
            if tab[j] < tab[min]:
                min = j
        
        tab[i], tab[min] = tab[min], tab[i]

# Przykład użycia
tab = [64, 25, 12, 22, 11]
selection_sort(tab)
print("Posortowana tablica:", tab)
